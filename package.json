{
	"name": "watchly-rn",
	"version": "1.0.0",
	"main": "expo-router/entry",
	"scripts": {
		"android": "expo start --android",
		"ios": "expo start --ios",
		"format": "bunx biome format --write",
		"generate-api-types": "bunx openapi-typescript http://localhost:3000/api/openapi/json -o ./types/api.ts",
		"build:ios": "eas build --platform ios --profile production --local",
		"build:preview:ios": "bunx eas-cli build --platform ios --profile preview",
		"build:preview:android": "bunx eas-cli build --platform android --profile preview",
		"build:production:android": "bunx eas-cli build --platform android --profile production",
		"submit:android": "bunx eas-cli submit --platform android"
	},
	"dependencies": {
		"@better-auth/expo": "^1.4.6",
		"@better-auth/stripe": "^1.4.9",
		"@biomejs/biome": "2.3.8",
		"@expo-google-fonts/bebas-neue": "^0.4.1",
		"@stripe/stripe-react-native": "0.50.3",
		"@tanstack/react-query": "^5.74.4",
		"better-auth": "^1.0.0",
		"expo": "~54.0.30",
		"expo-auth-session": "~7.0.10",
		"expo-constants": "^18.0.12",
		"expo-font": "^14.0.10",
		"expo-linking": "~8.0.11",
		"expo-localization": "^17.0.8",
		"expo-network": "^8.0.8",
		"expo-router": "~6.0.21",
		"expo-secure-store": "^15.0.8",
		"expo-splash-screen": "~31.0.13",
		"expo-status-bar": "~3.0.9",
		"expo-web-browser": "~15.0.10",
		"lucide-react-native": "^0.487.0",
		"openapi-fetch": "^0.15.0",
		"openapi-react-query": "^0.5.1",
		"react": "19.1.0",
		"react-error-boundary": "^6.0.0",
		"react-native": "0.81.5",
		"react-native-feather": "^1.1.2",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-svg": "15.12.1"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@stylistic/eslint-plugin-js": "^4.2.0",
		"@types/react": "~19.1.10",
		"ajv": "^8.17.1",
		"babel-plugin-module-resolver": "^5.0.2",
		"openapi-typescript": "^7.10.1",
		"prettier": "^3.5.3",
		"typescript": "~5.9.2"
	},
	"private": true,
	"packageManager": "bun@1.0.0",
	"overrides": {
		"expo-linking": "~8.0.11"
	}
}
